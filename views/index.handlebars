<div>
    <h1 class="card z-depth-3 center-align">News Update from Los Angeles Times</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
           
          <h5>  <a href="{{this.link}}" class="waves-effect waves-light btn" id="btnSignUp">Read Article</a></h5>
           

        </div>

        {{/each}}
    </div>
</div>